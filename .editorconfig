# EditorConfig is awesome: http://EditorConfig.org

root = true

[*]
indent_style = tab
end_of_line = lf
charset = utf-8
trim_trailing_whitespace = true

[*.svg]
indent_style = space
indent_size = 2

[package.json]
indent_style = space
indent_size = 2

[*.ftl]
indent_style = space
indent_size = 4
trim_trailing_whitespace = false